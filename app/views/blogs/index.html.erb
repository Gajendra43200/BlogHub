

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @blogs.each do |blog| %>
      <tr>
        <td><%= blog.title %></td>
        <td><%= blog.content %></td>
        <td><%= link_to 'Show', blog_path(blog)%></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Back to blog', blogs_path %>
<%= link_to 'Back to users path ', users_path %>
